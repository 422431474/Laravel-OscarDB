{
  "name": "microestc/oscardb",
  "description": "OscarDB driver for Laravel 8, 9, 10",
  "keywords": ["oscar", "laravel", "laravel 8", "laravel 9", "laravel 10", "pdo_aci", "aci", "oscardb", "microestc", "shentong"],
  "license": "MIT",
  "authors": [
      {
        "name": "Microestc",
        "email": "microestc@outlook.com"
      }
  ],
  "type": "library",
  "require": {
    "php": "^7.4|^8.0|^8.1|^8.2",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "illuminate/database": "^8.24.0|^9.0|^10.0",
    "illuminate/pagination": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.4.2",
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Microestc\\": "src/Microestc"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
          "Microestc\\OscarDB\\OscarDBServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-master": "1.2-dev"
    }
  },
  "minimum-stability": "dev"
}
